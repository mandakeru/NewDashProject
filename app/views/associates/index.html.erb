<p id="notice"><%= notice %></p>

<h1>Listing Associates</h1>

<table>
  <thead>
    <tr>
      <th>Associate number</th>
      <th>Association date</th>
      <th>Payment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @associates.each do |associate| %>
      <tr>
        <td><%= associate.associate_number %></td>
        <td><%= associate.association_date %></td>
        <td><%= associate.payment %></td>
        <td><%= link_to 'Show', associate %></td>
        <td><%= link_to 'Edit', edit_associate_path(associate) %></td>
        <td><%= link_to 'Destroy', associate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Associate', new_associate_path %>
